
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="keywords" content="Türkiye adres API, İl bilgisi API, İlçe bilgisi API, Mahalle bilgisi API, Sokak bilgisi API, Ücretsiz adres API, Adres veritabanı API, Web scraping adres bilgisi, İlçe sokak mahalle API, Türkiye adres veritabanı, Adres doğrulama API, Türkiye mahalle sokak sorgulama, İlçe sorgulama API, Adres verisi entegrasyonu, Geliştiriciler için adres API, Anlık adres sorgulama, Adres API entegrasyonu, Türkiye ilçe sorgusu, Mahalle sorgusu API, Adres doğrulama hizmeti">

    <meta name="description" content="Türkiye'nin kapsamlı adres veritabanına erişim sağlayan ücretsiz API'miz ile il, ilçe, mahalle ve sokak bilgilerini anında keşfedin. Kullanıcı dostu ve hızlı yanıt süreleri ile verilerinizi entegre edin ve projelerinizde anlık adres bilgilerini kullanın. Web scraping ile elde edilen bu API, tamamen ücretsizdir ve geliştiricilerin iş süreçlerini hızlandırmayı hedefler.">

    <title>@ViewData["Title"]</title>
    <link
      rel="stylesheet"
      href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css"
    />
   
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/animate.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/toastr.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/main.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/index.css" asp-append-version="true" />
   <link id="favicon" rel="icon" type="image/x-icon" href="~/favicon.ico">

    @await RenderSectionAsync("Styles", required: false)

</head>
<body>
    <!-- LOADER START -->
    <div
      id="spinner"
      class="show bg-white position-fixed translate-middle w-100 vh-100 top-50 start-50 d-flex align-items-center justify-content-center"
    >
      <div
        class="spinner-border text-warning"
        style="width: 3rem; height: 3rem"
        role="status"
      >
        <span class="sr-only"></span>
      </div>
    </div>
    <!-- LOADER END -->
    <!-- NAVBAR START -->
    <navbar class="section-header header-1 sticky-navbar" data-bs-theme="light">
      <div class="container">
        <nav class="navbar navbar-expand-xl hover-menu">
          <div class="d-flex w-100 justify-content-between align-items-center">
            <a class="navbar-brand" asp-page="Index" aria-label="nav-brands">
              <img
                src="~/img/logo.jpg"
                class="logo-light img-fluid rounded-circle"
                alt="logo"
              />
            </a>
            <div class="d-none d-xl-block">
              <div class="d-flex gap-70 align-items-center">
                <ul class="gap-20 navbar-nav mb-2 me-2 mb-lg-0">

                <li class="nav-item  extra-bold"> <a class="nav-link @ViewData["isActiveHome"]" asp-page="Index">Ana Sayfa</a></li>


              
                  <li class="nav-item extra-bold"> <a class="nav-link @ViewData["isActiveAbout"]" asp-page="Hakkinda" >Yapımcı</a>  </li>
                  
                  
                  
                  
                  
                  
                  <li class="nav-item extra-bold"> <a href="#" class="nav-link "  data-bs-toggle="offcanvas"
                data-bs-target="#offcanvasmobile-menu"
                aria-controls="offcanvasmobile-menu"
                aria-expanded="false"
                aria-label="Toggle navigation">İletişim</a>  </li>
                </ul>
              </div>
            </div>
            <div class="d-flex gap-20 align-items-center search-and-offcanva">
           

              <!-- Toggler button -->
              <a
                href="#"
                class="right-but d-flex align-items-center"
                style="
                  color: #000000b3;
                  border: 2px solid black;
                  border-radius: 5px;
                "
                data-bs-toggle="offcanvas"
                data-bs-target="#offcanvasmobile-menu"
                aria-controls="offcanvasmobile-menu"
                aria-expanded="false"
                aria-label="Toggle navigation"
              >
                <i
                  style="color: #000000b3; display: flex; align-items: center"
                  class="bi bi-text-right right-tog p-2"
                ></i>
              </a>
            </div>
          </div>
        </nav>
      </div>
    </navbar>
    <!-- NAVBAR END -->
    
     <!-- OFFCANVAS MENU START-->
    <div class="offcanvas offcanvas-end " tabindex="-1" id="offcanvasmobile-menu" aria-labelledby="offcanvasmobile-menuLabel">
        <div class="offcanvas-header">
            <h5 class="offcanvas-title fs-4" id="offcanvasmobile-menuLabel">Menü</h5>
            <hr>
            <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
        </div>
        <div class="offcanvas-body">

            <div class="author-about mb-40 ">
                        <div class="text-center">
                            <div class="author-image">
                                <a asp-page="Hakkinda"><img style="height: 100px; width:; border-radius:50%" src="~/img/ilker2.jpeg" alt="author-image"></a>
                            </div>
                            <h5 class="mt-3" ><a class="author-name" asp-page="Hakkinda">Mustafa İlker Kamış</a></h5>
                            <p class=" text-center">Bilgisayar Mühendisi</p>
                        </div>
                    </div>
                    <hr class="text-center w-50 align-self-center">
            <h4  class="offcanvas-title mb-30">Takip Et</h4>
             <div class="d-flex align-items-center justify-content-end">
                        <a href="https://github.com/ilkerCoder/" class="me-2"><i class="bi-2x bi bi-github pt-2"></i></a>
                        <a href="https://www.instagram.com/xpetra_ichor/"class="me-2" ><i class="bi bi-instagram"></i></a>
                        <a href="https://www.linkedin.com/in/mustafa-ilker-kami%C5%9F-268b551bb/" ><i class="bi bi-linkedin"></i></a>
                    </div>
            <ul class="navbar-nav justify-content-end flex-grow-1 pe-3">
                <li class="nav-item">
                    <a class="nav-link active" asp-page="Index">Ana Sayfa</a>
                </li>
                <hr>
                <!-- Kategoriler Dropdown Menü -->
             
                <li class="nav-item">
                    <a class="nav-link" asp-page="Hakkinda" >Yapımcı</a>
                </li>
                <hr>
           
             

            </ul>
        </div>
    </div>
    <!-- OFFCANVAS MENU END-->

  
        <main role="main" class="pb-3" >
            <partial name="_Notification">

            @RenderBody()
        </main>


    <!-- BACK-TO-TOP BUTTON START -->
    <div class="scroll_to_top show">
      <i class="bi bi-arrow-up-short" style="color: white"></i>
    </div>
    <!-- BACK-TO-TOP BUTTON END -->
 

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

    <script src="~/lib/wow/wow.min.js"></script>
    <script src="~/js/main.js" asp-append-version="true"></script>
   <script>
    new WOW().init();
    </script> 
     @await RenderSectionAsync("Scripts", required: false)
</body>
</html>


